<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>scimex.deamon</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/share/scimex/scimex</string>
        <string>/Users/{{USER}}</string>
    </array>
    <key>StandardOutPath</key>
    <string>/var/log/scimex.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/scimex.error.log</string>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
